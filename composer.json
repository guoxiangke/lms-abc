{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "highideas/laravel-users-online": "^3.0",
        "jellybool/flysystem-upyun": "^1.0",
        "kris/laravel-form-builder": "^1.30",
        "laravel-notification-channels/facebook": "^0.1.0",
        "laravel-notification-channels/twilio": "^2.1",
        "laravel/framework": "^6.0",
        "laravel/horizon": "^3.7",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.3",
        "laravelista/comments": "^3.3",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.1",
        "mtvs/eloquent-hashids": "^1.1",
        "overtrue/laravel-pinyin": "~4.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "simshaun/recurr": "^4.0",
        "socialiteproviders/facebook": "dev-master",
        "socialiteproviders/weixin": "^4.0",
        "spatie/laravel-activitylog": "^3.9",
        "spatie/laravel-medialibrary": "^7.19",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-query-builder": "^2.8",
        "spatie/laravel-schemaless-attributes": "^1.7",
        "yansongda/laravel-notification-wechat": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-er-diagram-generator": "^1.1",
        "beyondcode/laravel-query-detector": "^1.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "php-cs-fixer fix",
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}

{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "^4.0",
        "simplesoftwareio/simple-sms": "3.1.*",
        "jellybool/flysystem-upyun": "^1.0",
        "kris/laravel-form-builder": "^1.20",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "laravelista/comments": "^2.1",
        "league/oauth2-client": "^2.4",
        "lokielse/omnipay-wechatpay": "^3.0",
        "mercuryseries/flashy": "^1.0",
        "mews/captcha": "^2.2",
        "nicolasbeauvais/flysystem-onedrive": "^1.0",
        "omnipay/paypal": "^3.0",
        "overtrue/laravel-pinyin": "~4.0",
        "owen-it/laravel-auditing": "^9.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "simshaun/recurr": "^4.0",
        "socialiteproviders/facebook": "dev-master",
        "socialiteproviders/weixin": "^4.0",
        "spatie/laravel-backup": "^6.1",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.36",
        "torann/laravel-repository": "^0.5.10",
        "yansongda/laravel-notification-wechat": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.4",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-er-diagram-generator": "^1.1",
        "beyondcode/laravel-query-detector": "^1.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
